  <div class="row">
      <div class="col-xs-10 col-xs-offset-1">
      <% @lists.each do |list| %>
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title"><%= link_to list.name,list_path(list)%> - <small><%= list.description %></small><%= link_to "open_in_browser", list.website, class: "material-icons pull-right", target:"_blank"%></h3>
        </div>
        <div class="panel-body">
          <%= render partial: 'post' , collection: list.posts.reverse_order.limit(2), locals:{list: list} %>
        </div>
      </div><!-- panel panel-primary-->
      <% end %>
      <%= will_paginate @posts %>
    </div><!-- col-sm-10 col-sm-offset-2-->
  </div>
