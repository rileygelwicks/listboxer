<% @lists.each do |list| %>
  <h3><%= link_to list.name,list_path(list)%></h3>
  <h5><%= list.description %></h5>
  <p>The latest posts:</p>
  <% list.posts.each do |post| %>
    <%= link_to post.subject, list_post_path(list, post)%>
    <%= link_to 'Delete', list_post_path(list, post),method: :delete%>
    <br>
  <%end%>
  <br>
  <small><%= link_to "Open Website", list.website%></small>
  <button><%= link_to 'Edit', edit_list_path(list) %></button>
  <button><%= link_to 'Delete List', list_path(list), method: :delete %></button>
<% end %>
